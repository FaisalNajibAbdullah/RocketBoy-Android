<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.najib.rocketboy"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="14" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="26.0.0-alpha1" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/roketbg"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.najib.rocketboy.RocketBoyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="org.droidfoot.DroidfootActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="org.droidfoot.HelpActivity"
            android:label="@string/title_activity_help" >
        </activity>
        <activity
            android:name="org.droidfoot.SettingsActivity"
            android:label="@string/title_activity_settings" >
        </activity>
    </application>

</manifest>